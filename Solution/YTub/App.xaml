<Application x:Class="YTub.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:yTub="clr-namespace:YTub"
             StartupUri="Views\MainWindow.xaml">
    <Application.Resources>
         <ResourceDictionary>
            <yTub:ViewModelLocator x:Key="ViewModelLocator"></yTub:ViewModelLocator>
             <ResourceDictionary.MergedDictionaries>
                 <ResourceDictionary Source="ItemsStyles.xaml"></ResourceDictionary>
             </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
